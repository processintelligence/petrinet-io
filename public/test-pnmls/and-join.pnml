<?xml version="1.0" encoding="UTF-8"?>
<pnml>
    <net id="ptnet1" type="http://www.pnml.org/version-2009/grammar/ptnet">
        <page id="top-level">
            <transition id="t1">
                <graphics>
                    <position x="100" y="150" />
                    <dimension x="70" y="70" />
                </graphics>
                <name>
                    <text>Task A</text>
                </name>
            </transition>
            <transition id="t2">
                <graphics>
                    <position x="100" y="250" />
                    <dimension x="70" y="70" />
                </graphics>
                <name>
                    <text>Task B</text>
                </name>
            </transition>
            <place id="p1">
                <graphics>
                    <position x="250" y="200" />
                    <dimension x="50" y="50" />
                </graphics>
                <name>
                    <text>End</text>
                </name>
            </place>
            <arc id="a1" source="t1" target="p1">
                <graphics></graphics>
            </arc>
            <arc id="a2" source="t2" target="p1">
                <graphics></graphics>
            </arc>
        </page>
    </net>
</pnml>

