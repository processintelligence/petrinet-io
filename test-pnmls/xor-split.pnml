<?xml version="1.0" encoding="UTF-8"?>
<pnml>
    <net id="ptnet1" type="http://www.pnml.org/version-2009/grammar/ptnet">
        <page id="top-level">
            <!-- Place before transition A -->
            <place id="p1">
                <graphics>
                    <position x="50" y="200" />
                    <dimension x="50" y="50" />
                </graphics>
                <name>
                    <text></text>
                    <graphics>
                        <offset x="20" y="20" />
                    </graphics>
                </name>
                <initialMarking>
                    <text>1</text>
                </initialMarking>
            </place>
            <!-- Transition A -->
            <transition id="t1">
                <graphics>
                    <position x="150" y="200" />
                    <dimension x="70" y="70" />
                </graphics>
                <name>
                    <text>A</text>
                    <graphics>
                        <offset x="0" y="0" />
                    </graphics>
                </name>
            </transition>
            <!-- Split place (XOR split) -->
            <place id="p2">
                <graphics>
                    <position x="300" y="200" />
                    <dimension x="50" y="50" />
                </graphics>
                <name>
                    <text></text>
                    <graphics>
                        <offset x="20" y="20" />
                    </graphics>
                </name>
            </place>
            <!-- Two output transitions -->
            <transition id="t2">
                <graphics>
                    <position x="450" y="150" />
                    <dimension x="70" y="70" />
                </graphics>
                <name>
                    <text>B</text>
                    <graphics>
                        <offset x="0" y="0" />
                    </graphics>
                </name>
            </transition>
            <transition id="t3">
                <graphics>
                    <position x="450" y="250" />
                    <dimension x="70" y="70" />
                </graphics>
                <name>
                    <text>C</text>
                    <graphics>
                        <offset x="0" y="0" />
                    </graphics>
                </name>
            </transition>
            <!-- Arc: place to transition A -->
            <arc id="a1" source="p1" target="t1">
                <graphics></graphics>
                <inscription>
                    <text></text>
                    <graphics>
                        <offset x="0" y="0" />
                    </graphics>
                </inscription>
            </arc>
            <!-- Arc: transition A to split place -->
            <arc id="a2" source="t1" target="p2">
                <graphics></graphics>
                <inscription>
                    <text></text>
                    <graphics>
                        <offset x="0" y="0" />
                    </graphics>
                </inscription>
            </arc>
            <!-- Arcs: split place to two transitions (XOR split) -->
            <arc id="a3" source="p2" target="t2">
                <graphics></graphics>
                <inscription>
                    <text></text>
                    <graphics>
                        <offset x="0" y="0" />
                    </graphics>
                </inscription>
            </arc>
            <arc id="a4" source="p2" target="t3">
                <graphics></graphics>
                <inscription>
                    <text></text>
                    <graphics>
                        <offset x="0" y="0" />
                    </graphics>
                </inscription>
            </arc>
        </page>
    </net>
</pnml>
