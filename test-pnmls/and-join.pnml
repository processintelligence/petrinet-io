<?xml version="1.0" encoding="UTF-8"?>
<pnml>
    <net id="ptnet1" type="http://www.pnml.org/version-2009/grammar/ptnet">
        <page id="top-level">
            <!-- Places before transitions A and B -->
            <place id="p1">
                <graphics>
                    <position x="100" y="150" />
                    <dimension x="50" y="50" />
                </graphics>
                <name>
                    <text></text>
                    <graphics>
                        <offset x="20" y="20" />
                    </graphics>
                </name>
                <initialMarking>
                    <text>1</text>
                </initialMarking>
            </place>
            <place id="p2">
                <graphics>
                    <position x="100" y="250" />
                    <dimension x="50" y="50" />
                </graphics>
                <name>
                    <text></text>
                    <graphics>
                        <offset x="20" y="20" />
                    </graphics>
                </name>
                <initialMarking>
                    <text>1</text>
                </initialMarking>
            </place>
            <!-- Transitions A and B -->
            <transition id="t1">
                <graphics>
                    <position x="200" y="150" />
                    <dimension x="70" y="70" />
                </graphics>
                <name>
                    <text>A</text>
                    <graphics>
                        <offset x="0" y="0" />
                    </graphics>
                </name>
            </transition>
            <transition id="t2">
                <graphics>
                    <position x="200" y="250" />
                    <dimension x="70" y="70" />
                </graphics>
                <name>
                    <text>B</text>
                    <graphics>
                        <offset x="0" y="0" />
                    </graphics>
                </name>
            </transition>
            <!-- Place after transitions A and B (before join) -->
            <place id="p3">
                <graphics>
                    <position x="350" y="150" />
                    <dimension x="50" y="50" />
                </graphics>
                <name>
                    <text></text>
                    <graphics>
                        <offset x="20" y="20" />
                    </graphics>
                </name>
            </place>
            <place id="p4">
                <graphics>
                    <position x="350" y="250" />
                    <dimension x="50" y="50" />
                </graphics>
                <name>
                    <text></text>
                    <graphics>
                        <offset x="20" y="20" />
                    </graphics>
                </name>
            </place>
            <!-- Transition C (AND join) -->
            <transition id="t3">
                <graphics>
                    <position x="500" y="200" />
                    <dimension x="70" y="70" />
                </graphics>
                <name>
                    <text>C</text>
                    <graphics>
                        <offset x="0" y="0" />
                    </graphics>
                </name>
            </transition>
            <!-- Arcs: places to transitions A and B -->
            <arc id="a1" source="p1" target="t1">
                <graphics></graphics>
                <inscription>
                    <text></text>
                    <graphics>
                        <offset x="0" y="0" />
                    </graphics>
                </inscription>
            </arc>
            <arc id="a2" source="p2" target="t2">
                <graphics></graphics>
                <inscription>
                    <text></text>
                    <graphics>
                        <offset x="0" y="0" />
                    </graphics>
                </inscription>
            </arc>
            <!-- Arcs: transitions A and B to places -->
            <arc id="a3" source="t1" target="p3">
                <graphics></graphics>
                <inscription>
                    <text></text>
                    <graphics>
                        <offset x="0" y="0" />
                    </graphics>
                </inscription>
            </arc>
            <arc id="a4" source="t2" target="p4">
                <graphics></graphics>
                <inscription>
                    <text></text>
                    <graphics>
                        <offset x="0" y="0" />
                    </graphics>
                </inscription>
            </arc>
            <!-- Arcs: two places to one transition C (AND join) -->
            <arc id="a5" source="p3" target="t3">
                <graphics></graphics>
                <inscription>
                    <text></text>
                    <graphics>
                        <offset x="0" y="0" />
                    </graphics>
                </inscription>
            </arc>
            <arc id="a6" source="p4" target="t3">
                <graphics></graphics>
                <inscription>
                    <text></text>
                    <graphics>
                        <offset x="0" y="0" />
                    </graphics>
                </inscription>
            </arc>
        </page>
    </net>
</pnml>
